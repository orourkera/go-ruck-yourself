# Configuration to disable Watch targets during Flutter development
# This prevents errors when building for simulators

# Exclude the Watch app target when building with Flutter
EXCLUDED_ARCHS[sdk=iphonesimulator*] = i386 x86_64 arm64
EXCLUDED_ARCHS[sdk=iphoneos*]_GRY_Watch_App = arm64 armv7 armv7s

# Skip installing the Watch app
SKIP_INSTALL_GRY_Watch_App = YES
SKIP_INSTALL_GRY_Watch_App_Container = YES
