// Configuration to enable Watch targets during Flutter builds

// These architectures should be included for proper Watch app deployment
EXCLUDED_ARCHS[sdk=iphonesimulator*] = i386 x86_64

// Ensure Watch app is included in the build
SKIP_INSTALL_GRY_Watch_App = NO
SKIP_INSTALL_GRY_Watch_App_Container = NO

// Include deployment target configuration
#include "watchos_deployment_target.xcconfig"
